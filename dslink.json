{
  "name": "dslink-java-v2-jdbc-h2",
  "version": "0.0.2",
  "description": "Database link for H2 databases",
  "main": "bin/dslink-java-v2-jdbc-h2",
  "configs": {
    "main-node": {
      "type": "string",
      "value": "org.iot.dsa.dslink.jdbc.h2.MainNode"
    },
    "log": {
      "help": "all, trace, debug, info, warn, error, off",
      "type": "enum",
      "value": "info"
    },
    "msgpack": {
      "help": "Whether or not to use msgpack.  False is helpful for debugging.",
      "type": "bool",
      "value": false
    },
    "broker": {
      "type": "url",
      "value": null
    },
    "log-file": {
      "help": "Only set when the link is being run standalone",
      "type": "path",
      "value": null
    },
    "key": {
      "help": "Public/private key pair used by the link.",
      "type": "path",
      "value": null
    },
    "name": {
      "help": "Overrides the name above.",
      "type": "string",
      "value": null
    },
    "nodes": {
      "help": "Path to the node configuration database file.",
      "type": "path",
      "value": null
    },
    "token": {
      "help": "Authentication token for the broker.",
      "type": "string",
      "value": null
    }
  }
}
